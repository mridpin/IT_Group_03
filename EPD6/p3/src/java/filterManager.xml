<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<!-- Author: ridao -->
 
<struts>
    <!-- Paquete para la gestion de los filtros -->
    <package name="filter" namespace="/filter" extends="struts-default">
        <action name="showOverdue" class="actions.parking.LoadAllParkingsActionSupport" method="showOverdue"> 
            <result name="success">/index/index.jsp</result>
        </action>        
        <action name="showNotOverdue" class="actions.parking.LoadAllParkingsActionSupport" method="showNotOverdue"> 
            <result name="success">/index/index.jsp</result>
        </action>        
    </package>
    <package name="search" namespace="/search" extends="struts-default">
        <action name="searchByPlate" class="actions.parking.LoadAllParkingsActionSupport" method="searchByPlate"> 
            <result name="success">/index/index.jsp</result>
        </action>       
        <action name="showParked" class="actions.parking.LoadAllParkingsActionSupport" method="showParked"> 
            <result name="success">/index/index.jsp</result>
        </action>       
    </package>
</struts>
