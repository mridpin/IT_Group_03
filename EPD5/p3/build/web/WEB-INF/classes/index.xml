<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<!-- Author: ridao -->
 
<struts>    
    <package name="login" namespace="/index" extends="struts-default">
        <!--Default page-->
        <action name="index" class="actions.LoadAllParkingsActionSupport"> 
            <result name="success">index/index.jsp</result>
        </action>        
    </package>
    <package name="edit" namespace="/" extends="struts-default">
        <action name="forwardToParkingEdit" class="actions.EditParkingActionSupport"> 
            <result name="success">parking/parking.jsp</result>
            <result name="error">index/index.jsp</result>
        </action>
        <action name="editParking" class="actions.EditParkingActionSupport" method="editParking"> 
            <result name="success">index/index.jsp</result>
            <result name="error">index/index.jsp</result>
        </action>
    </package>
</struts>